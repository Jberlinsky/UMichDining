<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
      package="com.davidwilemski.umichdining"
      android:versionName="1.2" android:versionCode="4">
    <application android:icon="@drawable/icon" android:label="@string/app_name">
        <activity android:name=".UmichDining"
                  android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>


		
		
		
		
		
    
<activity android:name="CampusTab"></activity>

<activity android:name="DayMeals"></activity>
<activity android:name="MealActivity"></activity>


<activity android:name="PrefsView">
<intent-filter>  
<action android:name="android.intent.action.VIEW"></action>  
<category android:name="android.intent.category.DEFAULT"></category>  
<category android:name="android.intent.category.BROWSABLE"></category>  
<data android:scheme="umichdining" android:host="twitter"></data>  
</intent-filter> 
</activity>
</application>
    <uses-sdk android:minSdkVersion="4" />
    
    

<uses-permission android:name="android.permission.INTERNET"></uses-permission>
</manifest> 
